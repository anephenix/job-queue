			"pre-commit": "npm run lint && npm t"
		}
	},
	"dependencies": {
		"redis": "^4.7.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.25.1",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.10.0",
		"@types/mocha": "^10.0.8",
		"eslint": "^9.10.0",
		"globals": "^15.9.0",
		"husky": "^9.1.6",
		"mocha": "^10.7.3",
		"mocha-lcov-reporter": "^1.3.0",
		"nyc": "^17.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.2"
	}
